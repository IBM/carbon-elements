{
  "name": "@carbon/themes",
  "description": "Themes for applying color in the Carbon Design System",
  "version": "0.0.1-beta.1",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": "https://github.com/IBM/carbon-elements/tree/master/packages/themes",
  "bugs": "https://github.com/IBM/carbon-elements/issues",
  "keywords": [
    "ibm",
    "elements",
    "carbon",
    "carbon-elements",
    "carbon-design-system"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf es lib umd scss/_mixins.scss scss/_theme.scss scss/_variables.scss",
    "build": "yarn clean && bundler bundle src/index.js --name CarbonThemes && node tasks/build.js && bundler check 'scss/*.scss'"
  },
  "dependencies": {
    "@carbon/colors": "0.0.1-beta.1"
  },
  "devDependencies": {
    "@carbon/cli-reporter": "0.0.1-beta.1",
    "@carbon/test-utils": "0.0.1-beta.1",
    "change-case": "^3.0.2",
    "fs-extra": "^7.0.1",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.3"
  }
}
